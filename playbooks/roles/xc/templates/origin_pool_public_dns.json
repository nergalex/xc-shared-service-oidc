{
  "metadata": {
    "name": "{{ extra_namespace_app }}-origin",
    "namespace": "{{ extra_volterra_namespace }}",
    "labels": {
      "owner": "{{ extra_owner_email.split('@')[0] }}",
      "project": "cloudbuilder"
    },
    "description": "NGINX workshop",
    "disable": false
  },
  "spec": {
    "origin_servers": [
      {
        "private_name": {
          "dns_name": "{{ extra_app.origin_dns }}",
          "refresh_interval": 300,
          "site_locator": {
            "virtual_site": {
              "tenant": "{{ extra_volterra.tenant.full }}",
              "namespace": "shared",
              "name": "re-europe",
              "kind": "virtual_site"
            }
          },
          "outside_network": {}
        },
        "labels": {}
      }
    ],
    "use_tls": {
      "use_host_header_as_sni": {},
      "tls_config": {
        "default_security": {}
      },
      "skip_server_verification": {},
      "no_mtls": {},
      "default_session_key_caching": {}
    },
    "port": 443,
    "same_as_endpoint_port": {},
    "healthcheck": [
      {
        "tenant": "{{ extra_volterra.tenant.full }}",
        "namespace": "{{ extra_volterra_namespace }}",
        "name": "my-custom-https",
        "kind": "healthcheck"
      }
    ],
    "loadbalancer_algorithm": "LB_OVERRIDE",
    "endpoint_selection": "LOCAL_PREFERRED"
  }
}