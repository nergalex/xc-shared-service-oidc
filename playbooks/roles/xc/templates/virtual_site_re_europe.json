{
  "metadata": {
    "name": "re-europe",
    "namespace": "shared",
    "labels": {
      "owner": "{{ extra_owner_email.split('@')[0] }}",
      "project": "cloudbuilder"
    },
    "description": "NGINX workshop",
    "disable": false
  },
  "spec": {
    "site_type": "REGIONAL_EDGE",
    "site_selector": {
      "expressions": [
        "ves.io/siteName in (ves-io-ams9-ams, ves-io-pa2-par, ves-io-pa4-par, ves-io-tn2-lon, ves-io-fr4-fra)"
      ]
    }
  }
}